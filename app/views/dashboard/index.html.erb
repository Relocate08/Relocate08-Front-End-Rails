<h1>Relocate</h1>
<h1>Welcome, <%= current_user.name %>!</h1>

<h2>Select an area to search!</h2>

<div class="location-search">
  <%= form_with  url: '/address', method: :get, local: true do |f| %>
    <%= f.text_field :location, placeholder: 'Enter Zipcode', min: 10000, max: 99950  %>
    <%= f.submit 'Search Location' %>
  <% end %>
</div>
