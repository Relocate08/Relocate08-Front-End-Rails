<!DOCTYPE html>
<html>
  <head>
    <title>RelocateFe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav class="topnav">
      <% if current_user %>
      <%= link_to "Dashboard", dashboard_path %>
      <%= link_to "Log Out", logout_path %>
      <% end %>
    </nav>

    <% flash.each do |type, message| %>
      <p><%= message %></p>
    <% end %>

    <%= yield %>
  </body>
</html>
